{
    "name": "Oddsjam Extension",
    "description": "Adds functionality to the oddsjam website",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "permissions": [
        "tabs",
        "storage",
        "debugger",
        "scripting",
        "alarms"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}